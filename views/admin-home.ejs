<h1 class="admin__heading admin__heading--primary">Avaleht</h1>
<form action="/api/home/images" method="post" enctype="multipart/form-data" class="admin__form" id="homeImages">
  <h3 class="admin__heading admin__heading--secondary">Pildid</h3>
  <div class="admin__img-group">

    <div class="admin__input__group">
      <h3 class="admin__heading admin__heading--tertiary">Päiseikoon</h3>
      <input type="file" name="favicon" id="favicon" class="admin__hidden-input file">
      <label for="favicon" class="admin__small-btn" id="faviconLabel">Vali fail</label>
      <p class="admin__input-placeholder" id="faviconPlaceholder"><%= data.staticImages[1].filename %></p>
      <img class="admin__icon" src="/img/Lendutõus.jpg" id="faviconIcon" alt="Päiseikoon">
    </div>
    <div class="admin__input__group">
      <h3 class="admin__heading admin__heading--tertiary">Logo</h3>
      <input type="file" name="logo" id="logo" class="admin__hidden-input file">
      <label for="logo" class="admin__small-btn" id="logoLabel">Vali fail</label>
      <p class="admin__input-placeholder" id="logoPlaceholder"><%= data.staticImages[1].filename %></p>
      <img class="admin__icon" src="/img/Lendutõus.jpg" id="logoIcon" alt="Logo">
    </div>
    <div class="admin__input__group">
      <h3 class="admin__heading admin__heading--tertiary">Taustapilt</h3>
      <input type="file" name="homeBackgroundImg" id="homeBackgroundImg" class="admin__hidden-input file">
      <label for="homeBackgroundImg" class="admin__small-btn" id="homeBackgroundImgLabel">Vali fail</label>
      <p class="admin__input-placeholder" id="homeBackgroundImgPlaceholder"><%= data.staticImages[1].filename %></p>
      <img class="admin__icon" src="/img/Lendutõus.jpg" id="homeBackgroundImgIcon" alt="Taustapilt">
    </div>
  </div>
  <div class="admin__btn-group">
    <button class="admin__btn" type="submit" name="button" id="homeImagesBtn">Salvesta</button>
  </div>
</form>

