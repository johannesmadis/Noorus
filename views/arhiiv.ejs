<%- include partials/head {pageTitle: pageTitle, paiseikoon: paiseikoon} -%>
<%- include partials/admin-popup -%>

<body class="page-admin">
  <div class="page-admin__fixed-box page-admin__fixed-box--left">
    <div class="page-admin__btn-box">
      <a href="/" class="page-admin__btn">Kodulehele</a>
      <a href="" class="page-admin__small-btn hide" id="boldBtn">RASVANE KIRI</a>
    </div>
  </div>
  <div class="page-admin__fixed-box page-admin__fixed-box--right">
    <div class="page-admin__btn-box">
      <a href="" class="page-admin__btn" id="logoutBtn">Logi välja</a>
    </div>
  </div>
  <nav class="page-admin__navigation">
    <ul class="page-admin__list">
      <li class="page-admin__item"><a href="/admin/avaleht" class="page-admin__link">Avaleht</a></li>
      <li class="page-admin__item"><a href="/admin/koorist" class="page-admin__link">Koorist</a></li>
      <li class="page-admin__item"><a href="/admin/sundmused" class="page-admin__link">Sündmused</a></li>
      <li class="page-admin__item"><a href="/admin/kontakt" class="page-admin__link">Kontakt</a></li>
      <li class="page-admin__item"><a href="/admin/pood" class="page-admin__link">Pood</a></li>
    </ul>
  </nav>
<h2 class="page-admin__title">Arhiiv</h2>
<h2 class="page-admin__title">Möödunud sündmused</h2>
<form action="/upload/moodunud" method="post" class="page-admin__form" id="moodunud">
<div class="page-admin__form-wrapper" id="moodunudForm">
  <% for (var a = 0; a < moodunudData.length; a++) { %>
  <div class="page-admin__subform moodunudSubform" name="moodunud<%= a + 1 %>Subform" id="moodunud<%= a + 1 %>Subform">
    <h2 class="page-admin__subtitle moodunudHeading" id="moodunud<%= a + 1 %>Heading" name="moodunud<%= a + 1 %>Heading">Sündmus <%= a + 1 %></h2>
    <div class="page-admin__form-box">
      <div class="page-admin__form-field">
        <h3 class="page-admin__heading">Plakat</h3>
        <input type="file" id="moodunud<%= a + 1 %>Plakat" name="moodunud<%= a + 1 %>Plakat" class="page-admin__hidden-input file moodunudPlakat">
        <label for="moodunud<%= a + 1 %>Plakat" class="page-admin__small-btn moodunudPlakatLabel" id="moodunud<%= a + 1 %>PlakatLabel" name="moodunud<%= a + 1 %>PlakatLabel">Vali fail</label>
        <p class="page-admin__input-placeholder moodunudPlakatPlaceholder" id="moodunud<%= a + 1 %>PlakatPlaceholder" name="moodunud<%= a + 1 %>PlakatPlaceholder"><%= plakatid[a].filename %></p>
        <img class="page-admin__icon moodunudPlakatImg" src="<%= plakatid[a].url %>" id="moodunud<%= a + 1 %>PlakatImg" name="moodunud<%= a + 1 %>PlakatImg" alt="Plakat <%= a + 1 %>">
      </div>
      <div class="page-admin__form-field">
        <h3 class="page-admin__heading">Pealkiri</h3>
        <p class="page-admin__text">EST</p>
        <input type="text" class="page-admin__input page-admin__input--bold input moodunudPealkiriEst" id="moodunud<%= a + 1 %>PealkiriEst" name="moodunud<%= a + 1 %>PealkiriEst" value="<%= moodunudData[a].pealkiri.est %>">
        <p class="page-admin__text">EN</p>
        <input type="text" class="page-admin__input page-admin__input--bold input moodunudPealkiriEn" id="moodunud<%= a + 1 %>PealkiriEn" name="moodunud<%= a + 1 %>PealkiriEn" value="<%= moodunudData[a].pealkiri.en %>">
      </div>
    </div>
    <div class="page-admin__form-box moodunudLoikKohtForm" id="moodunud<%= a + 1 %>LoikKohtForm" name="moodunud<%= a + 1 %>LoikKohtForm">
      <% for (var i = 0; i < moodunudKohtData[a].kohad.length; i++) { %>
      <div class="page-admin__form-field moodunud<%= a + 1 %>LoikKoht" id="moodunud<%= a + 1 %>LoikKoht<%= i + 1 %>" data-heading="Link">
        <h3 class="page-admin__heading moodunud<%= a + 1 %>LoikKohtHeading">Link <%= i + 1 %></h3>
        <p class="page-admin__text">EST (AEG JA KOHT)</p>
        <textarea class="page-admin__input page-admin__text-area page-admin__input--bold input moodunud<%= a + 1 %>LoikKohtEst" id="moodunud<%= a + 1 %>LoikKohtEst<%= i + 1 %>" name="moodunud<%= a + 1 %>LoikKohtEst<%= i + 1 %>" rows="1"><%- moodunudKohtData[a].kohad[i].est %></textarea>
        <p class="page-admin__text">EN (AEG JA KOHT)</p>
        <textarea class="page-admin__input page-admin__text-area page-admin__input--bold input moodunud<%= a + 1 %>LoikKohtEn" id="moodunud<%= a + 1 %>LoikKohtEn<%= i + 1 %>" name="moodunud<%= a + 1 %>LoikKohtEn<%= i + 1 %>" rows="1"><%- moodunudKohtData[a].kohad[i].en %></textarea>
        <p class="page-admin__text">LINK</p>
        <input type="text" class="page-admin__input input moodunud<%= a + 1 %>LoikKohtLink" id="moodunud<%= a + 1 %>LoikKohtLink<%= i + 1 %>" name="moodunud<%= a + 1 %>LoikKohtLink<%= i + 1 %>" value="<%= moodunudKohtData[a].kohad[i].link %>">
        <button class="page-admin__small-btn moodunudLoikKohtDeleteBtn moodunud<%= a + 1 %>LoikKohtDeleteBtn" type="button" id="moodunud<%= a + 1 %>LoikKohtDeleteBtn<%= i + 1 %>"
          name="moodunud<%= a + 1 %>LoikKohtDeleteBtn<%= i + 1 %>">Kustuta</button>
      </div>
      <% } %>
    </div>
    <div class="page-admin__form-box">
      <button class="page-admin__small-btn moodunudLoikKohtAddBtn" type="button" id="moodunud<%= a + 1 %>LoikKohtAddBtn">+ Lisa koht ja aeg +</button>
    </div>
    <div class="page-admin__form-box moodunudLoikForm" id="moodunud<%= a + 1 %>LoikForm" name="moodunud<%= a + 1 %>LoikForm">
      <% for (var i = 0; i < moodunudData[a].loigud.length; i++) { %>
      <div class="page-admin__form-field moodunud<%= a + 1 %>Loik" id="moodunud<%= a + 1 %>Loik<%= i + 1 %>" data-heading="Rida">
        <h3 class="page-admin__heading moodunud<%= a + 1 %>LoikHeading">Rida <%= i + 1 %></h3>
        <p class="page-admin__text">EST</p>
        <div type="text" contenteditable="true" class="page-admin__input page-admin__text-area input editable-input moodunud<%= a + 1 %>LoikEstEd" id="moodunud<%= a + 1 %>LoikEst<%= i + 1 %>Ed" name="moodunud<%= a + 1 %>LoikEst<%= i + 1 %>Ed"><%- moodunudData[a].loigud[i].est %></div>
        <input type="text" class="page-admin__input page-admin__hidden-input moodunud<%= a + 1 %>LoikEst" id="moodunud<%= a + 1 %>LoikEst<%= i + 1 %>" name="moodunud<%= a + 1 %>LoikEst<%= i + 1 %>" value="<%- moodunudData[a].loigud[i].est %>"></input>
        <p class="page-admin__text">EN</p>
        <div type="text" contenteditable="true" class="page-admin__input page-admin__text-area input editable-input moodunud<%= a + 1 %>LoikEnEd" id="moodunud<%= a + 1 %>LoikEn<%= i + 1 %>Ed" name="moodunud<%= a + 1 %>LoikEn<%= i + 1 %>Ed"><%- moodunudData[a].loigud[i].en %></div>
        <input type="text" class="page-admin__input page-admin__hidden-input moodunud<%= a + 1 %>LoikEn" id="moodunud<%= a + 1 %>LoikEn<%= i + 1 %>" name="moodunud<%= a + 1 %>LoikEn<%= i + 1 %>" value="<%- moodunudData[a].loigud[i].en %>"></input>
        <button class="page-admin__small-btn moodunudLoikDeleteBtn moodunud<%= a + 1 %>LoikDeleteBtn" type="button" id="moodunud<%= a + 1 %>LoikDeleteBtn<%= i + 1 %>" name="moodunud<%= a + 1 %>LoikDeleteBtn<%= i + 1 %>">Kustuta</button>
      </div>
      <% } %>
    </div>
    <div class="page-admin__form-box">
      <button class="page-admin__small-btn moodunudLoikAddBtn" type="button" id="moodunud<%= a + 1 %>LoikAddBtn">+ Lisa rida +</button>
    </div>
    <button class="page-admin__btn moodunudSubmitBtn" id="moodunud<%= a + 1 %>SubmitBtn" name="moodunud<%= a + 1 %>SubmitBtn" type="submit">Salvesta</button>
    <div class="page-admin__form-box">
      <button class="page-admin__small-btn moodunudDeleteBtn" type="button" id="moodunud<%= a + 1 %>DeleteBtn">Kustuta sündmus</button>
      <button class="page-admin__small-btn moodunudRestoreBtn" type="button" id="moodunud<%= a + 1 %>RestoreBtn">Taasta sündmus</button>
    </div>
  </div>
  <% } %>
</div>
</form>

</body>

<%- include partials/footer -%>
