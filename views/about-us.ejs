<%- include partials/head-en {pageTitle: pageTitle} -%>
<%- include partials/navigation-en {currentPage: currentPage, pealkirjadData: pealkirjadData} -%>
<nav class="sidenav">
  <ul class="sidenav__list">
    <li class="sidenav__item sidenav__special-item"><a href="#" class="sidenav__link"><%= pealkirjadData[0].en %></a></li>
    <li class="sidenav__item"><a href="#section-members" class="sidenav__link"><%= pealkirjadData[1].en %></a></li>
    <li class="sidenav__item"><a href="#section-conductors" class="sidenav__link"><%= pealkirjadData[2].en %></a></li>
    <li class="sidenav__item"><a href="#section-history" class="sidenav__link"><%= pealkirjadData[3].en %></a></li>
    <li class="sidenav__item"><a href="#section-media" class="sidenav__link"><%= pealkirjadData[4].en %></a></li>
    <li class="sidenav__item"><a href="#section-sponsors" class="sidenav__link"><%= pealkirjadData[5].en %></a></li>
  </ul>
</nav>
<div class="body-container">
  <header class="section-header">
    <div class="section-header__img"></div>
    <div class="section-header__text-box">
      <h2 class="heading--secondary heading--purple u-margin-bottom-big"><%= pealkirjadData[0].en %></h2>
      <h3 class="heading--tertiary heading--purple u-margin-bottom-big"><%= sissejuhatusData.pealkiri.en %></h3>
      <div class="section-header__paragraphs">
        <% for (var i = 0; i < sissejuhatusData.loigud.length; i++) {
          %> <p class="paragraph--small paragraph--purple"><%= sissejuhatusData.loigud[i].en %></p>
      <% } %>
      </div>
    </div>
  </header>
  <section class="section-liikmed" id="section-members">
    <div class="section-liikmed__container">
      <h2 class="heading--secondary heading--purple u-margin-bottom-big"><%= pealkirjadData[1].en %></h2>
      <div class="section-header__paragraphs">
        <% for (var i = 0; i < liikmedData.loigud.length; i++) {
          %> <p class="paragraph--small paragraph--purple"><%= liikmedData.loigud[i].en %></p>
      <% } %>
      </div>
      <div class="section-liikmed__text-box">
        <div class="section-liikmed__small-box">
          <h3 class="heading--tertiary heading--purple u-margin-bottom-big"><%= liikmedData.haaleruhmaNimed[0].en %></h3>
          <p class="paragraph--small paragraph--purple u-margin-bottom-big"><%= liikmedData.haaleruhmaTutvustused[0].en %></p>
        </div>
        <div class="section-liikmed__small-box">
          <h3 class="heading--tertiary heading--purple u-margin-bottom-big"><%= liikmedData.haaleruhmaNimed[1].en %></h3>
          <p class="paragraph--small paragraph--purple u-margin-bottom-big"><%= liikmedData.haaleruhmaTutvustused[1].en %></p>
        </div>
      </div>
      <div class="section-liikmed__text-box">
        <div class="section-liikmed__small-box">
          <h3 class="heading--tertiary heading--purple u-margin-bottom-big"><%= liikmedData.haaleruhmaNimed[2].en %></h3>
          <p class="paragraph--small paragraph--purple u-margin-bottom-big"><%= liikmedData.haaleruhmaTutvustused[2].en %></p>
        </div>
        <div class="section-liikmed__small-box">
          <h3 class="heading--tertiary heading--purple u-margin-bottom-big"><%= liikmedData.haaleruhmaNimed[3].en %></h3>
          <p class="paragraph--small paragraph--purple u-margin-bottom-big"><%= liikmedData.haaleruhmaTutvustused[3].en %></p>
        </div>
      </div>
      <div class="section-liikmed__smaller-box">
        <h3 class="heading--tertiary heading--purple u-margin-bottom-big"><%= liikmedData.lauljadPealkiri.en %></h3>
        <p class="paragraph--small paragraph--purple u-margin-bottom-big"><strong>Sopranos - </strong><%= liikmedData.sopranid %></p>
        <p class="paragraph--small paragraph--purple u-margin-bottom-big"><strong>Altos - </strong><%= liikmedData.aldid %></p>
        <p class="paragraph--small paragraph--purple u-margin-bottom-big"> <strong>Tenors - </strong><%= liikmedData.tenorid %></p>
        <p class="paragraph--small paragraph--purple u-margin-bottom-big"><strong>Basses/baritones - </strong><%= liikmedData.bassid %></p>
      </div>
      <div class="section-liikmed__smaller-box">
        <h3 class="heading--tertiary heading--purple u-margin-bottom-big"><%= liikmedData.vilistlastePealkiri.en %></h3>
        <p class="paragraph--small paragraph--purple"><%= liikmedData.vilistlasteNimekiri.en %></p>
      </div>
    </div>
  </section>
  <section class="section-dirigendid" id="section-dirigendid">
    <h2 class="heading--secondary heading--purple"><%= pealkirjadData[2].en %></h2>
    <% for (var a = 0; a < dirigendidData.length; a++) { %>
    <div class="section-dirigendid__container">
      <div class="section-dirigendid__text-box">
        <h3 class="heading--tertiary heading--purple u-margin-bottom-big"><%= dirigendidData[a].nimi %></h3>
        <% for (var i = 0; i < dirigendidData[a].loigud.length; i++) { %>
        <p class="paragraph--small paragraph--purple u-margin-bottom-small"><%= dirigendidData[a].loigud[i].en %></p>
        <% } %>
      </div>
      <div class="section-dirigendid__img" style="background-image: url('..<%= portreed[a].url %>')">
      </div>
    </div>
    <% } %>
  </section>
  <section class="section-ajalugu" id="section-history">
    <h2 class="heading--secondary heading--purple"><%= pealkirjadData[3].en %></h2>
    <div class="section-ajalugu__container">
      <h3 class="heading--tertiary heading--purple u-margin-bottom-big"><%= ajaluguSissekandedData.pealkiri.en %></h3>
      <ul class="list">
        <% for (var i = 0; i < ajaluguSissekandedData.sissekanded.length; i++) { %>
        <li class="list__item list__item--small"><strong><%= ajaluguSissekandedData.sissekanded[i].aasta %></strong> <%= ajaluguSissekandedData.sissekanded[i].en %></li>
        <% } %>
      </ul>
      <% for (var a = 0; a < ajaluguSektsioonidData.length; a++) { %>
      <div class="section-ajalugu__text-box">
        <h3 class="heading--tertiary heading--purple u-margin-bottom-big"><%= ajaluguSektsioonidData[a].pealkiri.en %></h3>
        <% for (var i = 0; i < ajaluguSektsioonidData[a].loigud.length; i++) { %>
        <p class="paragraph--small paragraph--purple u-margin-bottom-small"><%= ajaluguSektsioonidData[a].loigud[i].en %></p>
        <% } %>
      </div>
      <% } %>
    </div>
  </section>
  <section class="section-meedia" id="section-media">
    <h2 class="heading--secondary heading--purple"><%= pealkirjadData[4].en %></h2>
    <div class="section-meedia__container">
      <h3 class="heading--tertiary heading--purple u-margin-bottom-big"><%= meediaSissejuhatusData.pealkiri.en %></h3>
      <% for (var i = 0; i < meediaSissejuhatusData.loigud.length; i++) { %>
      <p class="paragraph--small paragraph--purple u-margin-bottom-small"><%= meediaSissejuhatusData.loigud[i].en %></p>
      <% } %>
      <h3 class="heading--tertiary heading--purple u-margin-top-big u-margin-bottom-big"><%= meediaVideodData.pealkiri.en %></h3>
      <% for (var i = 0; i < meediaVideodData.manustamislingid.length; i++) { %>
      <div class="section-meedia__iframe">
        <iframe class="iframe" src="<%= meediaVideodData.manustamislingid[i] %>" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
      </div>
      <% } %>
      <h3 class="heading--tertiary heading--purple u-margin-bottom-big"><%= meediaLingidData.pealkiri.en %></h3>
      <ul class="list">
        <% for (var i = 0; i < meediaLingidData.lingid.length; i++) { %>
        <li class="list__item list__item--small"><a href="<%= meediaLingidData.lingid[i].link %>" class="list__link"><%= meediaLingidData.lingid[i].en %></a></li>
        <% } %>
      </ul>
    </div>
  </section>
  <section class="section-toetajad" id="section-sponsors">
    <h2 class="heading--secondary heading--purple"><%= pealkirjadData[5].en %></h2>
    <div class="section-toetajad__container">
      <% for (var i = 0; i < toetajadData.length; i++) { %>
      <div class="section-toetajad__logo-box">
        <a href="<%= toetajadData[i].link %>"><img src="<%= toetajadData[i].url %>" alt="Logo <%= i + 1 %>" class="section-toetajad__logo"></a>
      </div>
      <% } %>
    </div>
  </section>
</div>
<%- include partials/footnav-en {pealkirjadData: pealkirjadData} -%>
<%- include partials/footer -%>
