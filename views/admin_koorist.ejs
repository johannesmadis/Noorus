      <h2 class="page-admin__subtitle dirigendidHeading" id="dirigendid<%= a + 1 %>Heading" name="dirigendid<%= a + 1 %>Heading">Dirigent <%= a + 1 %></h2>
      <div class="page-admin__form-box">
        <div class="page-admin__form-field">
          <h3 class="page-admin__heading">Portree</h3>
          <input type="file" id="dirigendid<%= a + 1 %>Portree" name="dirigendid<%= a + 1 %>Portree" class="page-admin__hidden-input file dirigendidPortree">
          <label for="dirigendid<%= a + 1 %>Portree" class="page-admin__small-btn dirigendidPortreeLabel" id="dirigendid<%= a + 1 %>PortreeLabel" name="dirigendid<%= a + 1 %>PortreeLabel">Vali fail</label>
          <p class="page-admin__input-placeholder dirigendidPortreePlaceholder" id="dirigendid<%= a + 1 %>PortreePlaceholder" name="dirigendid<%= a + 1 %>PortreePlaceholder"><%= portreed[a].filename %></p>
          <img class="page-admin__icon dirigendidPortreeImg" src="<%= portreed[a].url %>" id="dirigendid<%= a + 1 %>PortreeImg" name="dirigendid<%= a + 1 %>PortreeImg" alt="Portree <%= a + 1 %>">
        </div>
        <div class="page-admin__form-field">
          <h3 class="page-admin__heading">Nimi</h3>
          <input type="text" class="page-admin__input input dirigendidNimi" name="dirigendid<%= a + 1 %>Nimi" id="dirigendid<%= a + 1 %>Nimi" value="<%= dirigendidData[a].nimi %>">
        </div>
      </div>
      <div class="page-admin__form-box dirigendidLoikForm" id="dirigendid<%= a + 1 %>LoikForm" name="dirigendid<%= a + 1 %>LoikForm">
        <% for (var i = 0; i < dirigendidData[a].loigud.length; i++) { %>
        <div class="page-admin__form-field dirigendid<%= a + 1 %>Loik" id="dirigendid<%= a + 1 %>Loik<%= i + 1 %>" data-heading="Lõik">
          <h3 class="page-admin__heading dirigendid<%= a + 1 %>LoikHeading">Lõik <%= i + 1 %></h3>
          <p class="page-admin__text">EST</p>
          <textarea type="text" class="page-admin__input page-admin__text-area input dirigendid<%= a + 1 %>LoikEst" id="dirigendid<%= a + 1 %>LoikEst<%= i + 1 %>" name="dirigendid<%= a + 1 %>LoikEst<%= i + 1 %>"
            rows="1"><%= dirigendidData[a].loigud[i].est %></textarea>
          <p class="page-admin__text">EN</p>
          <textarea type="text" class="page-admin__input page-admin__text-area input dirigendid<%= a + 1 %>LoikEn" id="dirigendid<%= a + 1 %>LoikEn<%= i + 1 %>" name="dirigendid<%= a + 1 %>LoikEn<%= i + 1 %>"
            rows="1"><%= dirigendidData[a].loigud[i].en %></textarea>
          <button class="page-admin__small-btn dirigendidLoikDeleteBtn dirigendid<%= a + 1 %>LoikDeleteBtn" type="button" id="dirigendid<%= a + 1 %>LoikDeleteBtn<%= i + 1 %>" name="dirigendid<%= a + 1 %>LoikDeleteBtn<%= i + 1 %>">Kustuta</button>
        </div>
        <% } %>
      </div>
      <div class="page-admin__form-box">
        <button class="page-admin__small-btn dirigendidLoikAddBtn" type="button" id="dirigendid<%= a + 1 %>LoikAddBtn">+ Lisa lõik +</button>
        <button class="page-admin__small-btn dirigendidDeleteBtn" type="button" id="dirigendid<%= a + 1 %>DeleteBtn">Kustuta dirigent</button>
