<form action="/upload/dirigendid" method="post" enctype="multipart/form-data" class="page-admin__form" id="kooristDirigendid">
  <h3 class="page-admin__title">Dirigendid</h3>
  <% for (var a = 0; a < dirigendidData.length; a++) { %>
  <div class="page-admin__form-wrapper" id="dirigendid-form">
    <div class="page-admin__subform dirigendid-subform" name="dirigendid<%= a + 1 %>Subform" id="dirigendid<%= a + 1 %>Subform">
      <h2 class="page-admin__subheading">Dirigent <%= a + 1 %></h2>
      <div class="page-admin__form-box">
        <div class="page-admin__form-field">
          <h3 class="page-admin__heading">Portree</h3>
          <input type="file" id="portree<%= a + 1 %>" name="portree<%= a + 1 %>" class="page-admin__hidden-input file portree">
          <label for="portree<%= a + 1 %>" class="page-admin__small-btn" id="portree<%= a + 1 %>Label">Vali fail</label>
          <p class="page-admin__input-placeholder" id="portree<%= a + 1 %>Placeholder"><%= portreed[a].filename %></p>
          <img class="page-admin__icon" src="<%= portreed[a].url %>" id="portree<%= a + 1 %>Img" alt="Portree<%= a + 1 %>">
        </div>
        <div class="page-admin__form-field">
          <h3 class="page-admin__heading">Nimi</h3>
          <input type="text" class="page-admin__input input dirigendidNimi" name="dirigendid<%= a + 1 %>Nimi" id="dirigendid<%= a + 1 %>Nimi" value="<%= %>">
        </div>
      </div>
      <div class="page-admin__form-box dirigendidLoik-form" id="dirigendid<%= a + 1 %>Loik-form">
        <% for (var i = 0; i < dirigendidData[a].loigud.length; i++) { %>
        <div class="page-admin__form-field dirigendid<%= a + 1 %>Loik" id="dirigendid<%= a + 1 %>Loik<%= i + 1 %>" data-heading="Lõik">
          <h3 class="page-admin__heading dirigendid<%= a + 1 %>Loik-heading">Lõik <%= i + 1 %></h3>
          <p class="page-admin__text">EST</p>
          <textarea type="text" class="page-admin__input page-admin__text-area input dirigendid<%= a + 1 %>Loik-input-est" id="dirigendid<%= a + 1 %>LoikEst<%= i + 1 %>" name="dirigendid<%= a + 1 %>LoikEst<%= i + 1 %>" rows="1"></textarea>
          <p class="page-admin__text">EN</p>
          <textarea type="text" class="page-admin__input page-admin__text-area input dirigendid<%= a + 1 %>Loik-input-en" id="dirigendid<%= a + 1 %>LoikEn<%= i + 1 %>" name="dirigendid<%= a + 1 %>LoikEn<%= i + 1 %>" rows="1"></textarea>
          <button class="page-admin__small-btn dirigendid<%= a + 1 %>Loik-delete-btn" type="button" id="dirigendid<%= a + 1 %>LoikDeleteBtn<%= i + 1 %>" name="dirigendid<%= a + 1 %>LoikDeleteBtn<%= i + 1 %>">Kustuta</button>
        </div>
        <% } %>
      </div>
      <div class="page-admin__form-box">
        <button class="page-admin__small-btn dirigendidLoik-add-btn" type="button" id="dirigendid<%= a + 1 %>LoikAddBtn">Lisa lõik</button>
      </div>
      <div class="page-admin__form-box">
        <button class="page-admin__small-btn dirigendid-delete-btn" type="button" id="dirigendid<%= a + 1 %>DeleteBtn">Kustuta dirigent</button>
      </div>
      <% } %>
      <button class="page-admin__btn" id="dirigendidAddBtn">Lisa dirigent</button>
    </div>
  </div>
  <button class="page-admin__btn" type="submit" id="kooristDirigendidBtn">Salvesta</button>
</form>
