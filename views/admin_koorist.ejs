<form action="/upload/pealkirjad" method="post" enctype="multipart/form-data" class="page-admin__form" id="kooristPealkirjad">
  <h3 class="page-admin__title">Sektsioonipealkirjad</h3>
  <div class="page-admin__form-box" id="pealkiri-form">
    <% for (var i = 0; i < pealkirjadData.length; i++) { %>
    <div class="page-admin__form-field pealkiri" id="pealkiri<%= i + 1 %>" data-heading="Pealkiri">
      <h3 class="page-admin__heading pealkiri-heading">Pealkiri <%= i + 1 %></h3>
      <p class="page-admin__text">EST</p>
      <input type="text" class="page-admin__input input" id="pealkiriEst<%= i + 1 %>" name="pealkiriEst<%= i + 1 %>" value="<%= pealkirjadData[i].est %>">
      <p class="page-admin__text">EN</p>
      <input type="text" class="page-admin__input input" id="pealkiriEn<%= i + 1 %>" name="pealkiriEn<%= i + 1 %>" value="<%= pealkirjadData[i].en %>">
    </div>
    <% } %>
  </div>
  <button class="page-admin__btn" type="submit" id="pealkiriBtn">Salvesta</button>
</form>
<form action="/upload/sissejuhatus" method="post" enctype="multipart/form-data" class="page-admin__form" id="kooristSissejuhatus">
  <h3 class="page-admin__title">Sissejuhatus</h3>
  <div class="page-admin__form-box">
    <div class="page-admin__form-field">
      <h3 class="page-admin__heading">Avapilt</h3>
      <input type="file" id="avapilt" name="avapilt" class="page-admin__hidden-input file">
      <label for="avapilt" class="page-admin__small-btn" id="avapiltLabel">Vali fail</label>
      <p class="page-admin__input-placeholder" id="avapiltPlaceholder"></p>
      <img class="page-admin__icon" src="<%= avapilt.url %>" id="avapiltImg" alt="Avapilt">
    </div>
    <div class="page-admin__form-field">
      <h3 class="page-admin__heading">Pealkiri</h3>
      <p class="page-admin__text">EST</p>
      <input type="text" class="page-admin__input input" id="sissejuhatusPealkiriEst" name="sissejuhatusPealkiriEst" value="<%= sissejuhatusData.pealkiri.est %>">
      <p class="page-admin__text">EN</p>
      <input type="text" class="page-admin__input input" id="sissejuhatusPealkiriEn" name="sissejuhatusPealkiriEn" value="<%= sissejuhatusData.pealkiri.en %>">
    </div>
  </div>
  <div class="page-admin__form-box" id="sissejuhatusLoik-form">
    <% for (var i = 0; i < sissejuhatusData.loigud.length; i++) { %>
    <div class="page-admin__form-field sissejuhatusLoik" id="sissejuhatusLoik<%= i + 1 %>" data-heading="Lõik">
      <h3 class="page-admin__heading sissejuhatusLoik-heading">Lõik <%= i + 1 %></h3>
      <p class="page-admin__text">EST</p>
      <textarea type="text" class="page-admin__input page-admin__text-area input sissejuhatusLoik-input-est" id="sissejuhatusLoikEst<%= i + 1 %>" name="sissejuhatusLoikEst<%= i + 1 %>" value="<%= sissejuhatusData.loigud[i].est %>"
        rows="1"><%= sissejuhatusData.loigud[i].est %></textarea>
      <p class="page-admin__text">EN</p>
      <textarea type="text" class="page-admin__input page-admin__text-area input sissejuhatusLoik-input-en" id="sissejuhatusLoikEn<%= i + 1 %>" name="sissejuhatusLoikEn<%= i + 1 %>" rows="1"
        <%= sissejuhatusData.loigud[i].en %>><%= sissejuhatusData.loigud[i].en %></textarea>
      <button class="page-admin__small-btn sissejuhatusLoik-delete-btn" type="button" id="sissejuhatusLoikDeleteBtn<%= i + 1 %>" name="sissejuhatusLoikDeleteBtn<%= i + 1 %>">Kustuta</button>
    </div>
    <% } %>
  </div>
  <div class="page-admin__form-box">
    <button class="page-admin__small-btn" type="button" id="sissejuhatusLoikAddBtn">Lisa lõik</button>
  </div>
  <button class="page-admin__btn" type="submit" id="kooristSissejuhatusBtn">Salvesta</button>
</form>
